		</constructor-arg>
		<!-- 请求响应时间设置5分钟 -->
		<constructor-arg index="3">
			<map>
				<entry key="queryOrderByMerchantManage" value="300000"/>
				<entry key="exportOrderByMerchantManage" value="300000"/>
			</map>
		</constructor-arg>
	</bean>
	
