	<bean id="ticketService"  class="com.froad.cbank.coremodule.framework.expand.thrift.proxy.DynamicClientProxyFactory"  factory-method="createIface" scope="prototype">
		<constructor-arg index="0">
			<value>com.froad.thrift.service.TicketService</value>
		</constructor-arg>
		<constructor-arg index="1">
			<value>${thrift.order.host}</value>
		</constructor-arg>
		<constructor-arg index="2">
			<value>TicketService</value>
		</constructor-arg>
		<!-- 请求响应时间设置5分钟 -->
		<constructor-arg index="3">
			<map>
				<entry key="exportTickets" value="300000"/>
			</map>
		</constructor-arg>
	</bean>
	<bean id="orderQueryService"  class="com.froad.cbank.coremodule.framework.expand.thrift.proxy.DynamicClientProxyFactory"  factory-method="createIface" scope="prototype">
