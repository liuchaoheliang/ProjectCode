<?xml version="1.0" encoding="UTF-8" ?>  
	<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  		 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.froad.fft.persistent.api.ProductPresellDeliveryMapper">

	<resultMap type="ProductPresellDelivery" id="ProductPresellDeliveryResultMap">
		<id property="id" column="id" />
		<result property="createTime" column="create_time" />
		<result property="updateTime" column="update_time" />
		<result property="productPresellId" column="product_presell_id" />
		<result property="presellDeliveryId" column="presell_delivery_id" />

	</resultMap>
	<!-- 插入 -->
	<insert id="saveProductPresellDelivery" parameterType="ProductPresellDelivery"
		useGeneratedKeys="true" keyProperty="id">
		insert into
		fft_product_presell_delivery values(
		#{id},#{createTime},#{updateTime},#{productPresellId},#{presellDeliveryId}
		)
	</insert>

	<!-- 查询 -->
	<select id="selectByProductPresellId" parameterType="java.lang.Long"
		resultMap="ProductPresellDeliveryResultMap">
		select * from fft_product_presell_delivery where
		product_presell_id=#{ProductId}
	</select>


	<!-- 删除 -->
	<delete id="deleteById" parameterType="java.lang.Long">
		delete from
		fft_product_presell_delivery where id =#{id}
	</delete>


	<!-- 删除 -->
	<delete id="deleteByPresellId" parameterType="java.lang.Long">
		delete from
		fft_product_presell_delivery where product_presell_id =#{presellId}
	</delete>


</mapper>