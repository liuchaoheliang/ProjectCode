<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.froad.db.ahui.mappers.AdMapper">

    <resultMap type="Ad" id="adResultMap">
        <id property="id" column="id"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>

        <result property="title" column="title"/>
        <result property="type" column="type" typeHandler="com.froad.db.extend.EnumTypeHandler"/>
        <result property="beginTime" column="begin_time"/>
        <result property="endTime" column="end_time"/>
        <result property="content" column="content"/>
        <result property="path" column="path"/>
        <result property="link" column="link"/>
        <result property="isEnabled" column="is_enabled"/>
        <result property="isBlankTarge" column="is_blank_targe"/>
        <result property="clickCount" column="click_count"/>
        <result property="orderValue" column="order_value"/>
        <result property="adPositionId" column="ad_position_id"/>
        <result property="dataState" column="data_state" typeHandler="com.froad.db.extend.EnumTypeHandler"/>
    </resultMap>



    <!-- 查询 -->
    <select id="findAd" parameterType="Ad" resultMap="adResultMap">
		select * from fft_ad
	</select>
	


</mapper>