<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.froad.db.ahui.mappers.ShippingMapper">
	<resultMap type="Shipping" id="ShippingResultMap">
		<id property="id" column="id" />
		<result property="createTime" column="create_time" />
		<result property="updateTime" column="update_time" />

		<result property="transId" column="transId" />
		<result property="deliveryCorpId" column="delivery_corpId" />
		<result property="deliveryCorpName" column="delivery_corpName" />
		<result property="trackingNo" column="tracking_No" />
		<result property="operator" column="operator" />
		<result property="remark" column="remark" />
		<result property="shippingTime" column="shipping_time"/>
		<result property="receiptTime" column="receipt_time"/>

	</resultMap>

	 <!-- 查询 -->
    <select id="findShipping" parameterType="Shipping" resultMap="ShippingResultMap">
		select * from fft_shipping
	</select>

</mapper>