<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.froad.db.mysql.rp_mappers.BatchJobMapper">
	
	<resultMap type="BatchJob" id="BatchJobRM">
		<result property="jobId" column="job_id"/>
		<result property="jobName" column="job_name"/>
		<result property="inputFile" column="input_file"/>
		<result property="inputFileType" column="input_file_type"/>
		<result property="chunkSize" column="chunk_size"/>
		<result property="remark" column="remark"/>
	</resultMap>
	
	<select id="findAll" resultMap="BatchJobRM">
		SELECT job_id, job_name, input_file, input_file_type, chunk_size, remark 
		FROM cb_report_batch_job
	</select>
</mapper>