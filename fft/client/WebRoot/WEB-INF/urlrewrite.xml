<?xml version="1.0" encoding="utf-8"?> 
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN" 
"http://tuckey.org/res/dtds/urlrewrite2.6.dtd">  
<urlrewrite> 


	<rule> 
		<from>^/([a-zA-Z0-9]*).html</from> 
		
		<to type="forward">/$1.action</to> 
	</rule>

	<rule>

	    <from>^/([a-zA-Z0-9]*)/([a-zA-Z0-9]*).html</from>
	
	    <to type="forward">/$1/$2.jsp</to>

    </rule>
	
	<rule>
	
		<from>^/(\w+)/([a-zA-Z0-9]*)_([a-zA-Z0-9]*).html</from>
		
		<to type="forward">/$1.action?$2=$3</to>
	
	</rule>
	
	<rule>
	
		<from>^/(\w+)/([a-zA-Z0-9]*)_([a-zA-Z0-9]*)/([a-zA-Z0-9]*)_([a-zA-Z0-9]*).html</from>
		
		<to type="forward">/$1.action?$2=$3&amp;$4=$5</to>
	
	</rule>
	
		
	
	<rule>
	
		<from>^/(\w+)/([a-zA-Z0-9]*)_([a-zA-Z0-9]*)/([a-zA-Z0-9]*)_([a-zA-Z0-9]*)/([a-zA-Z0-9]*)_([a-zA-Z0-9]*).html</from>
		
		<to type="forward">/$1.action?$2=$3&amp;$4=$5&amp;$6=$7</to>
	
	</rule>
	


	
	
</urlrewrite> 
