<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="presellDeliveryMap">
	<typeAlias alias="presellDeliveryMap" type="com.froad.CB.po.PresellDeliveryMap" />	
	<resultMap class="presellDeliveryMap" id="presellDeliveryMapResult">
		<result property="id" column="id" jdbcType="INTEGER"/>
		<result property="presellRackId" column="presell_rack_id" jdbcType="INTEGER"/>
		<result property="goodsPresellRack" column="presell_rack_id" select="goodsPresellRack.selectByPrimaryKey"/>
		<result property="deliveryId" column="delivery_id" jdbcType="INTEGER"/>
		<result property="presellDelivery" column="delivery_id" select="presellDelivery.getById"/>
	</resultMap>
		
	<insert id="insert" parameterClass="presellDeliveryMap">
		insert into presell_delivery_map (presell_rack_id,delivery_id) values(
			#presellRackId#,#deliveryId#
		)		
		<selectKey keyProperty="id" resultClass="int">
			select last_insert_id() as id
		</selectKey>
	</insert>
	
	<update id="updateById" parameterClass="presellDeliveryMap" >
		update presell_delivery_map 
		<dynamic prepend="set">
			<isNotEmpty property="presellRackId" prepend=",">
				presell_rack_id=#presellRackId#
			</isNotEmpty>
			<isNotEmpty property="deliveryId" prepend=",">
				delivery_id=#deliveryId#
			</isNotEmpty>		
		</dynamic>		
			where id=#id#	
	</update>
	
	<select id="getById" parameterClass="java.lang.Integer" resultMap="presellDeliveryMapResult">
		select * from presell_delivery_map where id=#id#	
	</select>
	
	<select id="getByConditions" parameterClass="presellDeliveryMap" resultMap="presellDeliveryMapResult">
		select * from presell_delivery_map 
		<dynamic prepend="where">
			<isNotEmpty property="id" prepend="and">
				id=#id#
			</isNotEmpty>
			<isNotEmpty property="deliveryId" prepend="and">
				delivery_id=#deliveryId#
			</isNotEmpty>
			<isNotEmpty property="presellRackId" prepend="and">
				presell_rack_id=#presellRackId#
			</isNotEmpty>		
		</dynamic>	
	</select>
	
	<select id="getByPager" parameterClass="presellDeliveryMap" resultMap="presellDeliveryMapResult">
		select * from presell_delivery_map 
		<dynamic prepend="where">
			<isNotEmpty property="id" prepend="and">
				id=#id#
			</isNotEmpty>
			<isNotEmpty property="deliveryId" prepend="and">
				delivery_id=#deliveryId#
			</isNotEmpty>
			<isNotEmpty property="presellRackId" prepend="and">
				presell_rack_id=#presellRackId#
			</isNotEmpty>		

		</dynamic>
			    <![CDATA[
            	limit #startRow#,#pageSize#  
            ]]> 
	</select>
	
	<select id="getByPagerCount" parameterClass="presellDeliveryMap" resultClass="java.lang.Integer">
		select count(*) from presell_delivery_map 
		<dynamic prepend="where">
			<isNotEmpty property="id" prepend="and">
				id=#id#
			</isNotEmpty>
			<isNotEmpty property="deliveryId" prepend="and">
				delivery_id=#deliveryId#
			</isNotEmpty>
			<isNotEmpty property="presellRackId" prepend="and">
				presell_rack_id=#presellRackId#
			</isNotEmpty>		

		</dynamic>
	</select>

	
	
	<delete id="deleteByRackId"  parameterClass="java.lang.Integer" >
		delete from presell_delivery_map where presell_rack_id=#id#
	</delete>	

</sqlMap>