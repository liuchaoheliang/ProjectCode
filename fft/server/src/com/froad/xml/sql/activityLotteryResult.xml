	<insert id="insert" parameterClass="activityLotteryResult">
		insert into activity_lottery_result_info (id, user_id, merchant_id, lottery_cust_id,
		type, securities_no, is_consume, consume_time, expire_time,
		sms_number, sms_time,price,point,goods_id, state, create_time, update_time, remark)
		values (#id:INTEGER#, #userId:VARCHAR#, #merchantId:VARCHAR#,
		#lotteryCustId:VARCHAR#, #type:VARCHAR#, #securitiesNo:VARCHAR#,
		#isConsume:VARCHAR#, #consumeTime:VARCHAR#,
		#expireTime:VARCHAR#, #smsNumber:INTEGER#, #smsTime:VARCHAR#,#price:VARCHAR#, #point:INTEGER#, #goodsId:VARCHAR#,
		#state:VARCHAR#, DATE_FORMAT(NOW(),"%Y-%m-%d|%H:%i:%s"), DATE_FORMAT(NOW(),"%Y-%m-%d|%H:%i:%s"),
		#remark:VARCHAR#)
		<selectKey keyProperty="id" resultClass="int">
			select last_insert_id() as id
		</selectKey>
	</insert>
	
	<update id="updateStateById" parameterClass="java.util.HashMap">
