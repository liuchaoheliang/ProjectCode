<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="sellerChannel">
	
	<typeAlias alias="sellerChannel" type="com.froad.CB.po.SellerChannel" />

	<resultMap class="sellerChannel" id="sellerChannelResult">
		<result property="id" column="id"/>
		<result property="userId" column="user_id"/>
		<result property="merchantId" column="merchant_id"/>
		<result property="sellerId" column="seller_id"/>
		<result property="sellerRuleId" column="seller_rule_id"/>
		<result property="sellerRule" column="seller_rule_id" select="sellerRule.selectByPrimaryKey"/>
		<result property="channelId" column="channel_id"/>
		<result property="fundsChannel" column="channel_id" select="fundsChannel.selectByPrimaryKey"/>
		<result property="isDefault" column="is_default"/>
		<result property="accountName" column="account_name"/>
		<result property="accountNumber" column="account_number"/>		
		<result property="state" column="state"/>
		<result property="createTime" column="create_time"/>
		<result property="updateTime" column="update_time"/>
		<result property="remark" column="remark"/>
	</resultMap>
	
	<!-- 添加卖家收款渠道信息 -->
	<insert id="addSellerChannel" parameterClass="sellerChannel">
		<![CDATA[ 
				insert into seller_channel(id,user_id,merchant_id,seller_id,seller_rule_id,channel_id,is_default,account_name,account_number,state,create_time,update_time,remark) 
				values(#id#,#userId#,#merchantId#,#sellerId#,#sellerRuleId#,#channelId#,#isDefault#,#accountName#,#accountNumber#,#state#,DATE_FORMAT(NOW(),"%Y-%m-%d|%H:%i:%s"),DATE_FORMAT(NOW(),"%Y-%m-%d|%H:%i:%s"),#remark#)
		]]>
			<selectKey keyProperty="id" resultClass="int">
				select last_insert_id() as id
			</selectKey>
	</insert>
	
	<!-- 更新卖家收款渠道信息  -->
	<update id="updateSellerChannelById" parameterClass="sellerChannel" >
	    <![CDATA[ 
	    		update seller_channel a 
	    		set a.update_time = DATE_FORMAT(NOW(),"%Y-%m-%d|%H:%i:%s")
	    ]]>
		<dynamic>
		     <isNotEmpty prepend="," property="userId">
		         <![CDATA[  a.user_id = #userId# ]]>
		     </isNotEmpty> 
		     <isNotEmpty prepend="," property="merchantId">
		         <![CDATA[  a.merchant_id = #merchantId# ]]>
		     </isNotEmpty> 
		     <isNotEmpty prepend="," property="sellerId">
		         <![CDATA[  a.seller_id = #sellerId# ]]>
		     </isNotEmpty> 
		     <isNotEmpty prepend="," property="sellerRuleId">
		         <![CDATA[  a.seller_rule_id = #sellerRuleId# ]]>
		     </isNotEmpty> 
		     <isNotEmpty prepend="," property="channelId">
		         <![CDATA[  a.channel_id = #channelId# ]]>
		     </isNotEmpty> 
		     <isNotEmpty prepend="," property="isDefault">
		         <![CDATA[  a.is_default = #isDefault# ]]>
		     </isNotEmpty> 
		     <isNotEmpty prepend="," property="accountName">
		         <![CDATA[  a.account_name = #accountName# ]]>
		     </isNotEmpty> 
		     <isNotEmpty prepend="," property="accountNumber">
		         <![CDATA[  a.account_number = #accountNumber# ]]>
		     </isNotEmpty> 
		     <isNotEmpty prepend="," property="state">
		         <![CDATA[  a.state = #state# ]]>
		     </isNotEmpty> 
		     <isNotEmpty prepend="," property="remark">
		         <![CDATA[  a.remark = #remark# ]]>
		      </isNotEmpty> 
		</dynamic>
		<![CDATA[ where a.id = #id# ]]>
	</update>
	
	<!-- 根据id物理删除卖家收款渠道信息 -->
	<delete id="deleteByPrimaryKey" parameterClass="sellerChannel" >
	    <![CDATA[ 
    			delete from seller_channel where id = #id#
	     ]]>
	</delete>
	
	<!-- 根据id删除卖家收款渠道信息 -->
	<update id="deleteStateByPrimaryKey" parameterClass="sellerChannel" >
	    <![CDATA[ 
    			update seller_channel a 
		     	set 
		     	a.update_time = DATE_FORMAT(NOW(),"%Y-%m-%d|%H:%i:%s"),a.state = '50' 
		     	where a.id = #id:INT#
	     ]]>
	</update>
	
	<!-- 根据userId查询卖家收款渠道信息 -->
	<select id="getSellerChannelByUserId" parameterClass="string" resultMap="sellerChannelResult">
		select * from seller_channel a where a.user_id=#userId#
	</select>
	
	<!-- 根据merchantId查询卖家收款渠道信息 -->
	<select id="getSellerChannelByMerchantId" parameterClass="string" resultMap="sellerChannelResult">
		select * from seller_channel a where a.merchant_id=#merchantId#
	</select>
	
	<!-- 根据sellerId查询卖家收款渠道信息 -->
	<select id="getSellerChannelBySellerId" parameterClass="string" resultMap="sellerChannelResult">
		select * from seller_channel a where a.seller_id=#sellerId#
	</select>
	
	<!-- 根据主键编号查询卖家收款渠道信息 -->
	<select id="selectByPrimaryKey" parameterClass="java.lang.Integer" resultMap="sellerChannelResult">
		select * from seller_channel a where a.id=#id#
	</select>
	
</sqlMap>