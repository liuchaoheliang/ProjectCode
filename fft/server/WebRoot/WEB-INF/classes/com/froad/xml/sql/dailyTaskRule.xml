<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="dailyTaskRule">
	<typeAlias alias="dailyTaskRule"
		type="com.froad.CB.po.DailyTaskRule" />
	
	<resultMap class="dailyTaskRule" id="dailyTaskRuleResultMap">
		<result property="id" column="id" jdbcType="INT" />
		<result property="fate" column="fate" jdbcType="VARCHAR" />
		<result property="isContinuation" column="is_continuation"
			jdbcType="VARCHAR" />
		<result property="pointValue" column="point_value"
			jdbcType="VARCHAR" />
		<result property="state" column="state" jdbcType="VARCHAR" />
		<result property="createTime" column="create_time"
			jdbcType="VARCHAR" />
		<result property="updateTime" column="update_time"
			jdbcType="VARCHAR" />
		<result property="remark" column="remark" jdbcType="VARCHAR" />
	</resultMap>
	
	<select id="getDailyTaskRuleByPrimaryId" parameterClass="java.lang.Integer" resultMap="dailyTaskRuleResultMap">
		select * from daily_task_rule
		where id=#id:INT#
	</select>
	
</sqlMap>