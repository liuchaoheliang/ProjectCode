<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="merchantClerkUrl">
	<typeAlias alias="merchantClerkUrl" type="com.froad.CB.po.MerchantClerkUrl"/>
	
	<resultMap id="MerchantClerkUrlResult" class="merchantClerkUrl">
		<result column="id" jdbcType="INTEGER" property="id" />
		<result column="url_type" property="urlType" jdbcType="VARCHAR" />
		<result column="url" property="url" jdbcType="VARCHAR" />
		<result column="state" jdbcType="VARCHAR" property="state" />
		<result column="create_time" jdbcType="VARCHAR" property="createTime" />
		<result column="update_time" jdbcType="VARCHAR" property="updateTime" />
		<result column="remark" jdbcType="VARCHAR" property="remark" />
	</resultMap>
	
	<!-- 增加商户操作组 -->
	<insert id="addMerchantClerkUrl" parameterClass="merchantClerkUrl">
		insert into merchant_clerk_url
		(id,url_type,url,state,create_time,update_time,remark) 
		values
		(#id#,#urlType#,#url#,#state#,DATE_FORMAT(NOW(),"%Y-%m-%d|%H:%i:%s"),DATE_FORMAT(NOW(),"%Y-%m-%d|%H:%i:%s"),#remark#)
		<selectKey keyProperty="id" resultClass="int">
			select last_insert_id() as id
		</selectKey>
	</insert>
	
	<select id="selectAll" resultMap="MerchantClerkUrlResult">
		select * from merchant_clerk_url
	</select>
	
	
</sqlMap>