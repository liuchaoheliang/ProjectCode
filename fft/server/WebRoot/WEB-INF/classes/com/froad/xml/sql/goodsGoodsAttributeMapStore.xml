<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="goodsGoodsAttributeMapStore">
	<typeAlias alias="goodsGoodsAttributeMapStore" type="com.froad.CB.po.GoodsGoodsAttributeMapStore" />
	
	<resultMap class="goodsGoodsAttributeMapStore" id="goodsGoodsAttributeMapStoreResult">
		<result property="goodsId" column="goods_id"/>
		<result property="goodsAttributeId" column="goods_attribute_id"/>
		<result property="element" column="element"/>
	</resultMap>
		
	 <!-- 增加商品属性存储 -->	
	<insert id="addGoodsGoodsAttributeMapStore" parameterClass="goodsGoodsAttributeMapStore">
		insert into goods_goods_attribute_map_store
		(goods_id,goods_attribute_id,element) 
		values
		(#goodsId#,#goodsAttributeId#,#element#)
	</insert>
    
</sqlMap>