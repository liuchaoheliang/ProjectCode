<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >

<sqlMap namespace="goodsRebateRackImages">
	<typeAlias alias="goodsRebateRackImages" type="com.froad.CB.po.GoodsRebateRackImages" />

	<resultMap class="goodsRebateRackImages" id="goodsRebateRackImagesResultMap">
		<result property="id" column="id" jdbcType="INT" />
		<result property="goodsRebateRackId"
			column="goods_rebate_rack_id" jdbcType="VARCHAR" />
		<result property="imagesUrl" column="images_url"
			jdbcType="VARCHAR" />
		<result property="state" column="state" jdbcType="VARCHAR" />
		<result property="createTime" column="create_time"
			jdbcType="VARCHAR" />
		<result property="updateTime" column="update_time"
			jdbcType="VARCHAR" />
		<result property="remark" column="remark" jdbcType="VARCHAR" />
	</resultMap>

	<insert id="insertObject" parameterClass="goodsRebateRackImages">
		insert into goods_rebate_rack_images (
		id,goods_rebate_rack_id,images_url,state,create_time,update_time,remark
		) values (
		#id:INTEGER#,#goodsRebateRackId:VARCHAR#,#imagesUrl:VARCHAR#,#state:VARCHAR#,
		DATE_FORMAT(NOW(),"%Y-%m-%d|%H:%i:%s"),DATE_FORMAT(NOW(),"%Y-%m-%d|%H:%i:%s"),#remark:VARCHAR#)

		<selectKey keyProperty="id" resultClass="int">
			select last_insert_id() as id
		</selectKey>
	</insert>
	
	<delete id="UpdateObject" parameterClass="goodsRebateRackImages">
		update goods_rebate_rack_images 
      	set state = #state:VARCHAR#
      	where id = #id:INT#
	</delete>
	
	<update id="updateObject" parameterClass="goodsRebateRackImages">
		update goods_rebate_rack_images
		set goods_rebate_rack_id = #goodsRebateRackId:VARCHAR#,images_url = #imagesUrl:VARCHAR#,
		state = #state:VARCHAR#,update_time = DATE_FORMAT(NOW(),"%Y-%m-%d|%H:%i:%s"),remark = #remark:VARCHAR#
      	where id = #id:INT#
	</update>
	
	<select id="getByPrimaryKey" parameterClass="java.lang.Integer" resultMap="goodsRebateRackImagesResultMap">
		select * from goods_rebate_rack_images where id = #id:INT#
	</select>
	
	<select id="getAll" resultMap="goodsRebateRackImagesResultMap">
		select * from goods_rebate_rack_images
	</select>
	
	<select id="getByGoodsRebateRackId" parameterClass="java.lang.String" resultMap="goodsRebateRackImagesResultMap">
		select * from goods_rebate_rack_images where goods_rebate_rack_id = #goodsRebateRackId:VARCHAR#
	</select>	
	
	
	
	


</sqlMap>