<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
	http://www.springframework.org/schema/context 
	http://www.springframework.org/schema/context/spring-context-3.2.xsd
	">
	
	<!-- 交易hessian 服务 -->
	<bean id="transService" class="org.springframework.remoting.caucho.HessianProxyFactoryBean">  
	    <property name="serviceUrl" value="${fft.hessian.server.url}/api/froad-fft/service/transExportService"/>  
	    <property name="serviceInterface" value="com.froad.fft.api.service.TransExportService"/>  
	</bean>
	

</beans>
